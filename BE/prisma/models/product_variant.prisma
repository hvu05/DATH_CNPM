model ProductVariant{
  id Int
  product_id Int
  color String? @db.Char(50)
  storage String? 
  import_price Int
  price Int
  quantity Int
  create_at DateTime

  @@id(name: "variant_id",[id, product_id])

  order_items OrderItem[]
  cart_items CartItem[]
  product Product @relation(fields: [product_id], references: [id])
}